{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Foundry",
			"type": "chrome",
			"request": "launch",
			"port": 9222,
			"url": "http://localhost:30000/game",
			"webRoot": "${fileDirname}",
			"runtimeArgs": [
				"--remote-debugging-port=9222",
				"--app=http://localhost:30000"
			],
			"cwd": "${fileDirname}",
			"sourceMapPathOverrides": {
				"webpack:///./*": "${fileDirname}/../*"
			}
		}
	]
}